set $mod Mod4
set $menu --no-startup-id rofi -show drun

# start dmenu (a program launcher)
bindsym $mod+d exec $menu

# A more modern dmenu replacement is rofi:

# Exit i3
bindsym $mod+Shift+plus exec "i3-nagbar -t warning -m 'You pressed the exit shortcut. Do you really want to exit i3? This will end your X session.' -B 'Yes, exit i3' 'i3-msg exit'"



# Screenshots
bindsym Print exec flameshot gui